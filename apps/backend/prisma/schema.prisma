generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model resources {
  id          String @unique(map: "sqlite_autoindex_resources_1")
  title       String @unique(map: "sqlite_autoindex_resources_2")
  description String @unique(map: "sqlite_autoindex_resources_3")
  link        String @unique(map: "sqlite_autoindex_resources_4")
  category    String @unique(map: "sqlite_autoindex_resources_5")
}
