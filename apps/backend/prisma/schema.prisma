generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model event {
  id String @id @unique @default(uuid())
  allDay Boolean?
  start DateTime
  end DateTime
  title String
  url String?
  color String?
}

model resources {
  id          String @id
  title       String
  description String
  link        String
  category    String
}

model dining {
  id          String   @id @default(uuid())
  name        String
  image       String
  address     String
  category    String
  contact     String?
  description String?
  website     String?
  hours       String?
  menu        String?
}
